def greatest(num1,num2,num3):
    if(num1>num2):
        R1 = num1
    else:
        R1 = num2
    if(R1>num3):
        print(f"{R1} is greatest")
    else:
        print(f"{num3} is greatest")

a = int(input("Enter first number :"))
b = int(input("Enter second number :"))
c = int(input("Enter third number :"))

greatest(a,b,c)
    
